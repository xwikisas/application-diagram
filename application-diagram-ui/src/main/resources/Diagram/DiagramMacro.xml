{{velocity output="false"}}
#macro (displayDiagram $reference)
  #set ($discard = $xwiki.ssx.use('Diagram.DiagramSheet'))
  #set ($showCaption = true)
  #if ($services.security.authorization.hasAccess('view', $reference))
    #set ($diagramDoc = $xwiki.getDocument($reference))
    #set ($diagramTitle = $diagramDoc.plainTitle)
    #set ($diagramObj = $diagramDoc.getObject('Diagram.DiagramClass'))
    #if ($diagramObj)
      #if ($xcontext.action == 'edit' || $xcontext.action == 'export' || $request.xpage == 'print'
        || $xcontext.macro.params.cached == 'true' || $request.outputSyntax=='annotatedhtml')
        #set ($pageIndex =  $xcontext.macro.params.pageIndex)
        #if ($pageIndex &gt;= 1)
          #getAttachmentName($pageIndex)
          #set ($svgFileName = "${attachmentName}.svg")
          #set ($pngFileName = "${attachmentName}.png")
          #if ($diagramDoc.getAttachment($svgFileName))
            #if ($xcontext.action == 'export' &amp;&amp; $diagramObj.getValue('exportUsingSVG') == 0
                &amp;&amp; $diagramDoc.getAttachment($pngFileName))
              #set ($diagramURL = $diagramDoc.getAttachmentURL($pngFileName, 'download', "v=$!diagramDoc.version"))
              #set ($output = "{{html clean='false'}}&lt;img src='$diagramURL' alt='$escapetool.xml($diagramTitle)'/&gt;{{/html}}")
            #else
              #set ($output = "{{html clean='false'}}#showDiagramSVGInHTML($diagramDoc, $svgFileName){{/html}}")
            #end
          #else
            ## Check if the png exists and display it otherwise throw an error.
            #if ($diagramDoc.getAttachment($pngFileName))
              #set ($diagramURL = $diagramDoc.getAttachmentURL($pngFileName, 'download', "v=$!diagramDoc.version"))
              #set ($output = "{{html clean='false'}}&lt;img src='$diagramURL' alt='$escapetool.xml($diagramTitle)'/&gt;{{/html}}")
            #else
              #set ($escapedErrorMessage = $escapetool.xml($services.localization.render('diagram.macro.invalidPageIndex')))
              #set ($output = "{{warning}}$escapedErrorMessage{{/warning}}")
            #end
          #end
        #else
          #set ($escapedErrorMessage = $escapetool.xml($services.localization.render('diagram.macro.invalidPageIndex')))
          #set ($output = "{{warning}}$escapedErrorMessage{{/warning}}")
        #end
      #else
        #set ($output = "{{display reference='$services.rendering.escape($reference, $xcontext.macro.doc.syntax)' /}}")
        #set ($showCaption = false)
      #end
    #else
      #set ($output = "{{warning}}$services.localization.render('diagram.macro.invalidReference'){{/warning}}")
    #end
  #else
    #set ($output = "{{warning}}$services.localization.render('diagram.macro.viewNotAllowed'){{/warning}}")
    #if ($reference.name == $services.model.getEntityReference('DOCUMENT', 'default'))
      #set ($diagramTitle = $reference.parent.name)
    #else
      #set ($diagramTitle = $reference.name)
    #end
  #end
  #if ($showCaption)
    (% class="diagram-container" %)(((
      (% class="thumbnail" %)(((
  #end
  $output
  #if ($showCaption)

        {{html clean="false"}}
        &lt;div class="caption"&gt;
          &lt;a class="diagram-title" href="$xwiki.getURL($reference)"
            title="$escapetool.xml($services.localization.render('diagram.macro.view'))"
            &gt;$escapetool.xml($diagramTitle)&lt;/a&gt;
          #if ($services.security.authorization.hasAccess('edit', $reference))
            &lt;a class="diagram-edit" href="$xwiki.getURL($reference, 'edit', $NULL)"
              title="$escapetool.xml($services.localization.render('diagram.macro.edit'))"
              target="_blank"&gt;$services.icon.renderHTML('pencil')&lt;/a&gt;
          #end
        &lt;/div&gt;
        {{/html}}
      )))
    )))
  #end
#end

#macro (getAttachmentName $index)
  #if ($index == 1)
    #set ($attachmentName = 'diagram')
  #else
    #set ($attachmentName = "diagram$index")
  #end
#end

#macro (createDiagram $reference)
  {{html clean="false"}}
  #set ($createURL = $xwiki.getURL($reference, 'edit', $escapetool.url({
    'template': 'Diagram.DiagramTemplate',
    'form_token': $escapetool.xml($services.csrf.token)
  })))
  &lt;div&gt;
    &lt;a class="diagram-create btn btn-default" href="$createURL"&gt;
      $escapetool.xml($services.localization.render('diagram.macro.create'))
    &lt;/a&gt;
  &lt;/div&gt;
  {{/html}}
#end

#macro (displayExternalDiagram $externalDiagramUrl)
    ## Need the setup done by the DiagramViewSheet, but we don't have a page reference for it to display.
    #set ($temporaryUploadSupported = ("$!services.temporaryAttachments" != ''))
    #set ($diagramConfig = {
      "debug": $services.debug.minify.equals(false),
      "locale": $xcontext.locale,
      "drawIOBasePath": $services.webjars.url('org.xwiki.contrib:draw.io', ''),
      "pdfImageExportZoom": $diagramObj.getValue('pdfImageExportZoom'),
      "disableExternalServices": $xwiki.getDocument('Diagram.DiagramConfig').getValue('disableExternalServices'),
      "isTemporaryUploadSupported": $temporaryUploadSupported
    })
    #if ($diagramConfig['disableExternalServices'] == 0)
      #if ($xcontext.action == 'view')
        #set ($discard = $xwiki.ssx.use('Diagram.DiagramSheet'))
        #set ($discard = $xwiki.jsx.use('Diagram.DiagramSheet'))
        #set ($discard = $xwiki.jsx.use('Diagram.DiagramViewSheet'))
        ## To be populated by the DiagramViewSheet javascript code.
        {{html}}
          &lt;div class="diagram"
            data-external-diagram-url="$escapetool.xml($!externalDiagramUrl)"
            data-diagram-config="$escapetool.xml($jsontool.serialize($diagramConfig))"
            data-title="$escapetool.xml($services.localization.render('diagram.macro.externalDiagramUrl.edit.notAllowed'))"
          &gt;&lt;span class="fa fa-spinner fa-spin"&gt;&lt;/span&gt;&lt;/div&gt;
        {{/html}}
      #else
        {{warning}}$services.rendering.escape($services.localization.render('diagram.macro.externalDiagramUrl.edit.previewNotSupported'), 'xwiki/2.1'){{/warning}}
      #end
    #else
      {{warning}}$services.rendering.escape($services.localization.render('diagram.macro.externalServicesDisabled'), 'xwiki/2.1'){{/warning}}
    #end
#end

#macro (executeDiagramMacro)
  #set ($discard = $xwiki.jsx.use('Diagram.DiagramMacro'))
  #set ($reference = $xcontext.macro.params.reference)
  #if ($stringtool.isEmpty($reference))
    #set ($reference = $services.model.createDocumentReference('Diagram', $doc.documentReference.parent))
  #else
    #set ($reference = $services.model.resolveDocument($reference))
  #end
  #if ("$!xcontext.macro.params.externalDiagramUrl" != '')
    #displayExternalDiagram($xcontext.macro.params.externalDiagramUrl)
  #elseif ($xwiki.exists($reference))
    #displayDiagram($reference)
  #elseif ($services.security.authorization.hasAccess('edit', $reference))
    #createDiagram($reference)
  #else
    {{warning}}$services.localization.render('diagram.macro.createNotAllowed'){{/warning}}
  #end
#end
{{/velocity}}

