#if ($services.licensing.licensor.hasLicensureForEntity($xcontext.macro.doc.documentReference))
  #executeDiagramMacro
#else
  {{missingLicenseMessage extensionName="diagram.extension.name"/}}
#end
{{/velocity}}</code>
